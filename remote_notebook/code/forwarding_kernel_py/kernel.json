{
  "display_name": "PySpark",
  "language": "python",
  "argv": [
    "/opt/conda/bin/python",
    "-m",
    "forwarding_kernel",
    "-f",
    "{connection_file}"
  ],
  "env": {
    "PYTHONPATH": "/usr/local/share/jupyter/kernels/pyspark:/opt/conda/lib/python3.7/site-packages",
    "KERNEL_NAME": "PythonExecutingKernel"
  }
}
